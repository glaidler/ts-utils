{
    "version": "0.0.1",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../js"
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./errors/ErrorContainer.ts",
        "./ui/Toolbars.ts",
        "./index.ts",
        "./typings/tsd.d.ts"
    ]
}